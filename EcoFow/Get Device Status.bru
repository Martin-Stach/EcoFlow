meta {
  name: Get Device Status
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/iot-open/sign/device/quota
  body: json
  auth: none
}

body:json {
  {
    "sn": {{deviceSerial}},
    "params": {
      "quotas": [
        "20_1.supplyPriority"
      ]
    }
  }
}

settings {
  encodeUrl: true
}
